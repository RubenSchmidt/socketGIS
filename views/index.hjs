<!DOCTYPE html>
<html>
<head>
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
    <link rel='stylesheet' href='/stylesheets/ol3-sidebar.min.css'/>
    <link rel="stylesheet" href="http://openlayers.org/en/master/css/ol.css" type="text/css">
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    <script type='text/javascript' src="https://cdn.socket.io/socket.io-1.2.0.js"></script>
    <script src="//api.tiles.mapbox.com/mapbox.js/plugins/turf/v2.0.0/turf.min.js"></script>
    <script>
        var socket = io();
    </script>
</head>
<body>
    <div id="sidebar" class="sidebar collapsed">
        <!-- Nav tabs -->
        <div class="sidebar-tabs">
            <ul role="tablist">
                <li><a href="#home" role="tab"><i class="fa fa-bars"></i></a></li>
                <li><a href="#profile" role="tab"><i class="fa fa-user"></i></a></li>
                <li><a href="#messages" role="tab"><i class="fa fa-envelope"></i></a></li>
            </ul>

            <ul role="tablist">
                <li><a href="#settings" role="tab"><i class="fa fa-gear"></i></a></li>
            </ul>
        </div>

        <!-- Tab panes -->
        <div class="sidebar-content">
            <div class="sidebar-pane" id="home">
                <h1 class="sidebar-header">
                    Verkt√∏y
                    <div class="sidebar-close"><i class="fa fa-caret-left"></i></div>
                </h1>
                <div id="tools">
                    <form class="form-inline">
                        <label>Geometry type &nbsp;</label>
                        <select id="type">
                            <option value="None">None</option>
                            <option value="Point">Point</option>
                            <option value="LineString">LineString</option>
                            <option value="Polygon">Polygon</option>
                            <option value="Circle">Circle</option>
                            <option value="Square">Square</option>
                            <option value="Box">Box</option>
                        </select>
                    </form>
                    <div>
                        <button id="bufferButton">Buffer</button>
                        <input type="number" id="bufferDistance">
                    </div>
                    <div>
                        <p>Slett valgte element</p>
                        <button id="deleteSelectedButton">Slett</button>
                    </div>
                </div>
            </div>

            <div class="sidebar-pane" id="profile">
                <h1 class="sidebar-header">Profile<div class="sidebar-close"><i class="fa fa-caret-left"></i></div></h1>
            </div>

            <div class="sidebar-pane" id="messages">
                <h1 class="sidebar-header">Messages<div class="sidebar-close"><i class="fa fa-caret-left"></i></div></h1>
                <ul id="chat_messages"></ul>
                <form id="chat-form" action="">
                    <input id="m" autocomplete="off" /><button>Send</button>
                </form>
            </div>

            <div class="sidebar-pane" id="settings">
                <h1 class="sidebar-header">Settings<div class="sidebar-close"><i class="fa fa-caret-left"></i></div></h1>
            </div>
        </div>
    </div>
    <div id="map" class="sidebar-map"></div>

    <script src="http://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="/javascripts/underscore-min.js"></script>
    <script src="http://openlayers.org/en/master/build/ol.js" type="text/javascript"></script>
    <script src="/javascripts/jquery-sidebar.js"></script>
    <script src="/javascripts/proj4-compressed.js"></script>
    <script src="/javascripts/SOSI.min.js"></script>
    <script src="/javascripts/map.js"></script>
    <script src="/javascripts/chat.js"></script>
</body>
</html>
